#include<math.h>
#include<stdio.h>

int main(){
	int a,b,c;
	float X1,X2;
	printf("Enter a:");
	scanf("%d",&a);
	printf("Enter b:");
	scanf("%d",&b);
	printf("Enter c:");
	scanf("%d",&c);
	  printf("For Question:%d X^2 + %d X + %d=0,two roots are\n",a,b,c);
	  if(b*b-4*a*c>=0){
	  	X1=(-b+sqrt(b*b-4*a*c))/(2*a);
		X2=(-b-sqrt(b*b-4*a*c))/(2*a);
		printf("X1=%.1f\nX2=%.1f",X1,X2); 
	  }else{
	  	printf("no rerl roots");
	  }
	  return 0;
}
